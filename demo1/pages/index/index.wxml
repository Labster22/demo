<swiper class="big"
  indicator-dots="{{indicatorDots}}"
  autoplay="{{autoplay}}"
  interval="{{interval}}"
  duration="{{duration}}"
 
>
  <block wx:for="{{imgUrls}}">
    <swiper-item>
      <image src="{{item}}"  class="image"/>
    </swiper-item>
  </block>
</swiper>
<button bindtap="changeIndicatorDots">indicator-dots</button>


